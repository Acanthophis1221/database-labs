mydb=# CREATE TABLE students (student_id SERIAL PRIMARY KEY, name VARCHAR(50), email VARCHAR(100), faculty VARCHAR(50));
CREATE TABLE
mydb=# INSERT INTO students (name, email, faculty) VALUES ('Alymbek', 'alymbek@example.com', 'COM-25'), ('Timur',   'timur@example.com',   'Engineering'), ('Beka',    'beka@example.com',    'MAT'), ('Aizada',  'aizada@example.com',  'MED'), ('Nursultan','nursultan@example.com','Engineering');
INSERT 0 5
mydb=# SELECT * FROM students;
