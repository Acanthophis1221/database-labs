mydb=# SELECT name, email FROM students ORDER BY name ASC;
   name    |         email
-----------+-----------------------
 Aizada    | aizada@example.com
 Alymbek   | alymbek@example.com
 Beka      | beka@example.com
 Nursultan | nursultan@example.com
 Timur     | timur@example.com
(5 ёЄЁюъ)


mydb=# SELECT name, email FROM students ORDER BY name DESC;
   name    |         email
-----------+-----------------------
 Timur     | timur@example.com
 Nursultan | nursultan@example.com
 Beka      | beka@example.com
 Alymbek   | alymbek@example.com
 Aizada    | aizada@example.com
(5 ёЄЁюъ)


mydb=# SELECT name, email FROM students LIMIT 2;
  name   |        email
---------+---------------------
 Alymbek | alymbek@example.com
 Timur   | timur@example.com
(2 ёЄЁюъш)


mydb=# -- This query retrieves all students from the Engineering faculty.
mydb=# SELECT name, faculty FROM students WHERE faculty = 'Engineering';
   name    |   faculty
-----------+-------------
 Timur     | Engineering
 Nursultan | Engineering
(2 ёЄЁюъш)


